package ${context.getProperty("namespace")}.client.validate;

import java.io.Serializable;

/**
 * Contains a message text
 */
public class ValidationErrorProxy implements Serializable {

    private String text;
    private String property;
    
    public ValidationErrorProxy() {
        
    }

    public ValidationErrorProxy(String text, String property) {
        this.text=text;
        this.property=property;
    }

    public String getText() {
        return text;
    }

    public String getProperty() {
        return property;
    }

}
